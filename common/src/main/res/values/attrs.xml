<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="yl_common_SeniorEditText">
        <!--图片资源   使用的资源图不能使用vector形成的图片否则你懂的-->
        <!--清除按钮图-->
        <attr name="yl_common_clearDrawable" format="reference"/>
        <!--可见按钮图-->
        <attr name="yl_common_visibleDrawable" format="reference"/>
        <!--不可见按钮图-->
        <attr name="yl_common_invisibleDrawable" format="reference"/>
        <!--按钮宽度大小-->
        <attr name="yl_common_visibleDrawableWidth" format="dimension"/>
        <!--按钮间距大小 （自己的内边距）-->
        <attr name="yl_common_visibleDrawableSpacing" format="dimension"/>
        <!--边框颜色-->
        <attr name="yl_common_editFrameColor" format="color"/>
        <!--边样式     rectangle(矩形)、roundRect(圆角)、animator(动画划线特效)、halfRect(半矩形样式)-->
        <!-- 不填写样式 默认使用系统样式 -->
        <attr name="yl_common_editFrameStyle" format="string" />
    </declare-styleable>
</resources>
